{
	"name": "sy-i18n",
	"displayName": "sy-i18n",
	"description": "翻译",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.60.0"
	},
	"publisher": "345398278",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "sy-i18n.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.提取中文",
				"title": "提取此文件中文"
			},
			{
				"command": "extension.翻译vue文件",
				"title": "翻译vue文件"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"submenu": "fileTemplate",
					"group": "navigation@1"
				}
			],
			"fileTemplate": [
				{
					"when": "editorFocus",
					"command": "extension.提取中文",
					"group": "navigation"
				},
				{
					"when": "editorFocus",
					"command": "extension.翻译vue文件",
					"group": "navigation"
				}
			]
		},
		"submenus": [
			{
				"id": "fileTemplate",
				"label": "翻译此文件"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.60.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"htmlparser2": "^7.1.1",
		"http-proxy": "^1.18.1",
		"md5": "^2.3.0",
		"posthtml-parser": "^0.10.1",
		"posthtml-render": "^3.0.0",
		"vue-template-compiler": "^2.6.14"
	}
}
